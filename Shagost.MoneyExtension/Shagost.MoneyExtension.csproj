<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

    <PackageId>Shagost.MoneyExtension</PackageId>
    <Version>1.0.0</Version>
    <License>MIT</License>
    <Authors>Silvio Agostinho</Authors>
    <PackageDescription>Extension to convert decimal to integer</PackageDescription>
    <Description>Extension to convert decimal to integer</Description>
    <RepositoryUrl>https://github.com/shagost/money-extensions</RepositoryUrl>
    <PackageProjectUrl>https://github.com/shagost/money-extensions</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>money.png</PackageIcon>
    <PackageTags>money;int;decimal;converter</PackageTags>
    <Title>Shagost.MoneyExtension</Title>
    <Summary>Extension to convert decimal to integer</Summary>
  </PropertyGroup>

  <ItemGroup>
    <None Include="../Media/money.png" Pack="true" PackagePath="\" />
  </ItemGroup>
  

</Project>
